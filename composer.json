    "description": "Execute scheduled jobs or tasks in parallel",
    "keywords": [
        "php",
        "cron",
        "scheduler",
        "schedule",
        "promise"
    ],
    "authors": [
        {
            "name": "Pavel",
            "email": "pavel.k.danilin@gmail.com"
        }
    ],
    "require": {
        "php":  "^7.4",
        "ext-pcntl": "*",
        "react/event-loop": "^1.2",
        "react/promise": "^v2.8.0",
        "react/stream": "^1.2",
        "opis/closure": "^3.6",
        "symfony/process": "^4.4|^5.3",
        "dragonmantank/cron-expression": "^3.1",
        "psr/log": "^1.1"
    },
    "require-dev": {
        "guzzlehttp/guzzle": "^7.0",
        "doctrine/dbal": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "Paveldanilin\\ProcessExecutor\\": "src"
        }
    }
